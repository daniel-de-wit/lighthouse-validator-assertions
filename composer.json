{
    "name": "daniel-de-wit/lighthouse-validator-assertions",
    "type": "library",
    "description": "PHPUnit assertions for validating Lighthouse validators",
    "keywords": [
        "pupunit",
        "lighhouse",
        "lighthouse-validator-test"
    ],
    "homepage": "https://github.com/daniel-de-wit/lighthouse-validator-assertions",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel de Wit",
            "email": "daniel@danieldewit.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "phpunit/phpunit": "^7.5 || ^8.4 || ^9.0 || ^10.0 || ^11.0",
        "nuwave/lighthouse": "^5.0 || ^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.7"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DanielDeWit\\LighthouseValidatorAssertions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DanielDeWit\\LighthouseValidatorAssertions\\Tests\\": "tests"
        }
    }
}
